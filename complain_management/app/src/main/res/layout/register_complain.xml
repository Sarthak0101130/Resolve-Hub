<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home_page_activity">

    <View
        android:id="@+id/add_user_top_bar_box"
        android:layout_width="412dp"
        android:layout_height="42dp"
        android:background="@drawable/add_user_select_address_proof_box_shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.104" />

    <TextView
        android:id="@+id/add_user_text"
        android:layout_width="359dp"
        android:layout_height="36dp"
        android:text="REGISTER COMPLAINT"
        android:textAlignment="center"
        android:textColor="@color/add_user_text_color"
        android:textSize="24sp"
        android:textStyle="bold"

        app:flow_verticalAlign="center"
        app:layout_constraintBottom_toBottomOf="@+id/add_user_top_bar_box"
        app:layout_constraintEnd_toEndOf="@+id/add_user_top_bar_box"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="@+id/add_user_top_bar_box"
        app:layout_constraintTop_toTopOf="@+id/add_user_top_bar_box"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>