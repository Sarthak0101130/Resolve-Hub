<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <View
        android:id="@+id/request_complaint_top_box"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/add_user_select_address_proof_box_shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.041"
        tools:layout_editor_absoluteY="42dp" />

    <TextView
        android:id="@+id/add_user_text"
        android:layout_width="335dp"
        android:layout_height="51dp"
        android:gravity="center"
        android:text="REQUEST COMPLAINT"
        android:textColor="@color/add_user_text_color"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/request_complaint_top_box"
        app:layout_constraintEnd_toEndOf="@+id/request_complaint_top_box"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="@+id/request_complaint_top_box"
        app:layout_constraintTop_toTopOf="@+id/request_complaint_top_box"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>